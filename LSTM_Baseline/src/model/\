import numpy as np
# this thing needs a few things

# normalization of E (the readings)

# one hot encoder for I,D,H (time of day, day of week, holiday)


# one hot encoder:
def one_hot_encoder(data):
    #find cardinality of data
    cardinality = max(data) - min(data)
    


def one_hot_encoder_elem(element, cardinality):
    vector = np.zeros(cardinality)
    vector[element] = 1

    return vector

